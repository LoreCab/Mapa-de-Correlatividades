@model Universidad.Web.Models.LoginModel
@{
    ViewBag.Title = "Login";
}

<style>
    body {
       background-image: url('../../autogestion-banner.jpg');
       background-repeat:no-repeat;
       background-size:cover;
    }

    footer 
    {
        position: fixed;
        bottom: 0;
        color:white;
        height:100px;
        font-weight: bold;
    }
</style>

<div class="page-login">
    @using (Html.BeginForm("Index", "Home", FormMethod.Post))
    {
        <img class="logo" src="data:image/jpeg;base64," />
        <div class="control">
            <label>USUARIO</label>
            @Html.TextBoxFor(x => x.DNI)
        </div>
        <div class="control">
            <label>CONTRASEÑA</label>
            @Html.PasswordFor(x => x.Password)
        </div>
        <button type="submit">Login</button>
    }

    
    <!--<hr />-->
    <footer>
        <p>
            &copy; @DateTime.Now.Year - CABALLERO, ANDREA LORENA
        </p>
    </footer>
    <!--</div>-->
</div>
